<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="ClearTextPhoneOrBank">
        <attr name="is_phone" format="boolean" />
        <attr name="is_show" format="boolean" />
        <attr name="hint_normal" format="boolean" />
    </declare-styleable>
    <attr name="firstColor" format="color" />
    <attr name="secondColor" format="color" />
    <attr name="circleWidth" format="dimension" />
    <attr name="circleCount" format="integer" />
    <attr name="circlesSpec" format="dimension" />
    <attr name="circleBg" format="reference" />
    <attr name="pointWidth" format="integer" />
    <attr name="pointColor" format="color" />
    <attr name="movePointColor" format="color" />
    <attr name="circleBgColor" format="color" />
    <attr name="secondTextColor" format="color" />
    <attr name="startAngle" format="integer" />
    <attr name="firstTextSize" format="dimension" />
    <attr name="secondTextSize" format="dimension" />
    <declare-styleable name="HalfCircleView">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="circleCount" />
        <attr name="circlesSpec" />
        <attr name="circleBg" />
        <attr name="pointWidth" />
        <attr name="pointColor" />
        <attr name="movePointColor" />
        <attr name="circleBgColor" />
    </declare-styleable>
    <declare-styleable name="BenthCircleView">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="startAngle" />
        <attr name="firstTextSize" />
        <attr name="secondTextSize" />
        <attr name="secondTextColor" />
    </declare-styleable>
    <declare-styleable name="Prompt_Button">
        <attr name="canTxChange" format="boolean" />
    </declare-styleable>
    <declare-styleable name="List_No_Data">
        <attr name="img" format="reference" />
        <attr name="content" format="string" />
        <attr name="content_desc" format="string" />
        <attr name="distance_top" format="dimension" />
    </declare-styleable>
    <declare-styleable name="two_radio_attrs">
        <attr name="leftname" format="string" />
        <attr name="rightname" format="string" />
    </declare-styleable>

    <!--我的页面顶部头像-->
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color" />

    </declare-styleable>

    <!-- 圆形进度条属性 -->
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundBgColor" format="color"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <!--<attr name="max" format="integer"></attr>-->
        <attr name="textIsDisplayable" format="boolean"/>
        <attr name="style">
            <enum name="STROKE" value="0"/>
            <enum name="FILL" value="1"/>
        </attr>
    </declare-styleable>
</resources>